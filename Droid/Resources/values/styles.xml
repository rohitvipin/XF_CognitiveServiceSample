<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="MyCustomTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
		<item name="android:actionBarStyle">@style/MyCustomTheme.ActionBarStyle</item>
		<item name="android:colorActivatedHighlight">@android:color/transparent</item>
	</style>
	<style name="MyCustomTheme.ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
		<item name="android:titleTextStyle">@style/MyCustomTheme.ActionBar.TitleTextStyle</item>
		<item name="android:background">@color/mygreen</item>
	</style>
	<style name="MyCustomTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
		<item name="android:textColor">@android:color/white</item>
	</style>
</resources>